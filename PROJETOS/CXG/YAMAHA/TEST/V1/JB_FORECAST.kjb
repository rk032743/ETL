    <entry>
      <name>UPDATE_LOG_FORECAST</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>UPDATE dba_db_adm.tb_log_atualizacao a
INNER JOIN (SELECT CURDATE() AS etl_data ,COALESCE(criado_em, NOW()) AS inicio, NOW() AS fim
			,COALESCE(TIMEDIFF(NOW(), criado_em), 0) AS duracao ,COUNT(*) AS registros
			,IF(COUNT(*) = 0, 'Erro', 'Completo') AS `status`
			FROM ${VAR_CXG_DB}.tb_forecast
			WHERE EXTRACT(YEAR_MONTH FROM data) = ${VAR_YEAR_MONTH}) b
SET a.etl_data = b.etl_data
, a.inicio = b.inicio
, a.fim = b.fim
, a.duracao = b.duracao
, a.registros = b.registros
, a.data_postagem = ${MDATE}
, a.status = b.status
, a.data = b.etl_data
, a.observacao = 'Forecast'
, a.processo = 'Forecast'
WHERE id = 47;</sql>

    <entry>
      <name>LOAD_FORECAST</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>DROP TABLE IF EXISTS tb_forecast_temp ;
CREATE TABLE tb_forecast_temp LIKE tb_forecast;

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
LOAD DATA LOCAL INFILE ${P_CARGA_FORECAST}
IGNORE INTO TABLE tb_forecast_temp
CHARACTER SET utf8mb4
FIELDS TERMINATED BY ';'
ENCLOSED BY '"'
LINES TERMINATED BY '\r\n'
IGNORE 1 LINES;
SET SESSION TRANSACTION ISOLATION LEVEL REPEATABLE READ;

REPLACE INTO tb_forecast
SELECT * FROM tb_forecast_temp;

DROP TABLE IF EXISTS tb_forecast_temp ;</sql>
